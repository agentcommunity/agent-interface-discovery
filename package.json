{
  "name": "agent-identity-discovery",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "packageManager": "pnpm@8.15.0",
  "license": "MIT",
  "author": "Agent Community",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentcommunity/agent-identity-discovery.git"
  },
  "engines": {
    "node": ">=20.10",
    "pnpm": ">=8.15.0"
  },
  "pnpm": {
    "overrides": {
      "glob": "^13.0.0",
      "qs": "^6.14.1",
      "@isaacs/brace-expansion": ">=5.0.1"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel --filter=!@agentcommunity/e2e-tests",
    "dev:core": "turbo run dev --parallel --filter=@agentcommunity/aid --filter=@agentcommunity/aid-doctor",
    "dev:web": "turbo run dev --filter=@agentcommunity/aid-web...",
    "dev:all": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:parity": "pnpm -C packages/aid test && (cd packages/aid-go && go test ./...) && python3 -m pip install -e \"packages/aid-py[dev,pka]\" && python3 -m pytest packages/aid-py",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "one-shot": "eslint --cache --max-warnings 0 .",
    "format": "prettier --write .",
    "gen": "tsx scripts/generate-constants.ts && tsx scripts/generate-examples.ts",
    "docs:check": "node scripts/docs-check.mjs",
    "docs:export": "node scripts/docs-export.mjs",
    "docs:verify": "pnpm docs:check && pnpm docs:export && git diff --exit-code packages/docs/export-manifest.json packages/docs/export-manifest.sha256",
    "e2e": "turbo run e2e",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.6.2",
    "typescript": "^5.9.3",
    "yaml": "^2.8.2"
  }
}
