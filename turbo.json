{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "node_modules/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "e2e": {
      "dependsOn": ["build", "^build"],
      "cache": false,
      "outputs": []
    }
  }
}
