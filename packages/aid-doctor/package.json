{
  "name": "@agentcommunity/aid-doctor",
  "version": "0.1.0",
  "description": "CLI tool for Agent Interface Discovery (AID) - validate and check AID records",
  "keywords": [
    "agent",
    "discovery",
    "cli",
    "dns",
    "aid",
    "doctor"
  ],
  "author": "Agent Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentcommunity/agent-interface-discovery.git",
    "directory": "packages/aid-doctor"
  },
  "homepage": "https://aid.agentcommunity.org",
  "bugs": {
    "url": "https://github.com/agentcommunity/agent-interface-discovery/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aid-doctor": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentcommunity/aid": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.30.1",
    "tsup": "^8.0.0",
    "typescript": "^5.8.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
