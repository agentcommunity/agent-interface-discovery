{
  "name": "@agentcommunity/aid-web-generator",
  "version": "0.1.0",
  "description": "CLI tool for generating web interfaces for Agent Interface Discovery",
  "keywords": [
    "agent",
    "discovery",
    "web",
    "generator",
    "cli"
  ],
  "author": "Agent Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentcommunity/agent-interface-discovery.git",
    "directory": "packages/web-generator"
  },
  "homepage": "https://aid.agentcommunity.org",
  "bugs": {
    "url": "https://github.com/agentcommunity/agent-interface-discovery/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aid-web-generator": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepack": "turbo run build --filter={workspace}"
  },
  "dependencies": {
    "@agentcommunity/aid": "workspace:*",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "inquirer": "^9.2.23"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/inquirer": "^9.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
